#Brute Force
class Solution:
    def containsDuplicate(self, nums: List[int]) -> bool:
        n=len(nums)
        for i in range(0, n-1):
                for j in range(i+1, n):
                        if nums[i] == nums[j]:
                            return True
        return False

#Bubble sort first then comparision   
class Solution:
    def containsDuplicate(self, nums: List[int]) -> bool:
        n = len(nums)
        for i in range(n-1):
            swaps = 0
            for j in range(0,n-i-1):
                if nums[j] > nums[j+1]:
                    nums[j], nums[j+1] = nums[j+1], nums[j]
                    swaps = 1
            if (swaps == 0):
                break
        
        for i in range(n-1):
            if nums[i] == nums[i+1]:
                return True
        return False
    
#using set
class Solution:
    def containsDuplicate(self, nums: List[int]) -> bool:
        new_nums = set(nums)
        if len(nums) != len(new_nums):
            return True
        else:
            return False
        
#using hashset
class Solution:
    def containsDuplicate(self, nums: List[int]) -> bool:
        hashset = set()
        for i in nums:
            if i in hashset:
                return True
            else:
                hashset.add(i)
        return False

# Optimal but verbose
class Solution:
    def isAnagram(self, s: str, t: str) -> bool:

        countS = {}
        countT = {}

        if len(s) != len(t):
            return False

        for i in range(len(s)):
            if s[i] in countS:
                countS[s[i]] = 1 + countS[s[i]]
            else:
                countS[s[i]] = 1
        for j in range(len(t)):    
            if t[j] in countT:
                countT[t[j]] = 1 + countT[t[j]]
            else:
                countT[t[j]] = 1 
        return countS == countT

#Optimal using .get()
class Solution:
    def isAnagram(self, s: str, t: str) -> bool:
        countS = {}
        countT = {}

        if len(s) != len(t):
            return False
        
        for i in range(len(s)):
            countS[s[i]] = countS.get(s[i], 0) + 1
            countT[t[i]] = countT.get(t[i], 0) + 1
        return countS == countT
